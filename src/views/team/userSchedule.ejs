<h1>Datas ocupadas</h1>
<div id="dates">
    <% for (let i=0; i < dates.length; i++) { %>
        <% let d=new Date(dates[i].date); %>
        <p>Time: <%= dates[i].name %></p>
        <p>Data:
            <%= d.getDate() + "/" + (d.getMonth() + 1) + "/" + d.getFullYear() + " " + d.getHours() + ":" +
                d.getMinutes() %>
        </p>
    <% } %>
</div>
