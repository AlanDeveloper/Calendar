<h1>Dashboard</h1>
<p>
    <%= user.name %>
</p>
<a href="/team/register">Criar time</a>
<a href="/team/list">Seus times</a>
<a href="/logout">Sair</a>

<h1>Times</h1>
<% for (let i=0; i < teams.length; i++) { %>
    <ul>
        <li>
            <div>
                <p><%= teams[i].name %></p>
                <a href="/team/participants?teamId=<%=teams[i].id%>">Ver participantes</a>
                <a href="#">Enviar convite</a>
            </div>
        </li>
    </ul>
<% } %>
